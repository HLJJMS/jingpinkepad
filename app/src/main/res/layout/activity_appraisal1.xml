<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_appraisal1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="smjj.pureclass_pad1.activity.AppraisalActivity1">


    <include
        android:id="@+id/head_actionbar"
        layout="@layout/head_toolbar" />

    <include
        android:id="@+id/rl_bottom_actionbar"
        layout="@layout/bottom_copyright_bar" />


    <LinearLayout
        android:id="@+id/ll_appraisal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:layout_above="@id/rl_bottom_actionbar"
        android:layout_below="@id/head_actionbar"
        android:orientation="vertical">


        <ImageView
            android:id="@+id/iv_appraisal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/appraisal2"/>

    </LinearLayout>

</RelativeLayout>
